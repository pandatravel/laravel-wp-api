{
    "name": "ammonkc/laravel-wp-api",
    "description": "A Laravel Client for Wordpress REST Api",
    "type": "library",
    "version": "0.2.0",
    "require": {
        "illuminate/support": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "guzzlehttp/guzzle": "^6.3",
        "vnn/wordpress-rest-api-client": "^1.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "vlucas/phpdotenv": "^2.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ammon Casey",
            "email" : "ammon@caseyohana.com",
            "role" : "Lead Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ammonkc\\WpApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ammonkc\\WpApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ammonkc\\WpApi\\WpApiServiceProvider"
            ],
            "aliases": {
                "WpApi": "Ammonkc\\WpApi\\Facade"
            }
        }
    }
}
